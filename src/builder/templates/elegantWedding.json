{
  "name": "Elegant Wedding",
  "slug": "elegant-wedding",
  "preview": "/previews/elegant-wedding.jpg",
  "description": "Romantic design with pastel pinks and champagne gold for wedding photographers",
  "tags": ["Wedding", "Elegant", "Romantic", "Luxury"],
  "globalStyles": {
    "colors": {
      "primary": "#fae8e8",
      "secondary": "#c8a97e",
      "text": "#4a4a4a",
      "background": "#fffaf7"
    },
    "fonts": {
      "heading": "Great Vibes",
      "body": "Montserrat"
    },
    "spacing": "relaxed",
    "borderRadius": "large"
  },
  "pages": [
    {
      "title": "Home",
      "slug": "home",
      "blocks": [
        {
          "type": "Hero",
          "props": {
            "headline": "Timeless Wedding Photography",
            "subtext": "Capturing your love story with elegance and grace",
            "bgImage": "/images/wedding-hero.jpg",
            "ctaText": "View Galleries",
            "ctaLink": "#galleries",
            "style": "carousel",
            "images": ["/images/wedding-1.jpg", "/images/wedding-2.jpg", "/images/wedding-3.jpg"]
          }
        },
        {
          "type": "Timeline",
          "props": {
            "title": "Your Wedding Journey",
            "events": [
              {
                "title": "Initial Consultation",
                "description": "We meet to discuss your vision and style",
                "icon": "chat"
              },
              {
                "title": "Engagement Session",
                "description": "Optional session to get comfortable with the camera",
                "icon": "camera"
              },
              {
                "title": "Wedding Day",
                "description": "Full coverage of your special day",
                "icon": "heart"
              },
              {
                "title": "Gallery Delivery",
                "description": "Receive your beautifully edited photos",
                "icon": "gift"
              }
            ]
          }
        },
        {
          "type": "Testimonial",
          "props": {
            "style": "slider",
            "testimonials": [
              {
                "quote": "Our wedding photos are absolutely breathtaking. Every moment was captured perfectly!",
                "client": "Sarah & Michael",
                "image": "/images/couple-1.jpg"
              },
              {
                "quote": "Professional, talented, and so easy to work with. Highly recommend!",
                "client": "Emma & James",
                "image": "/images/couple-2.jpg"
              }
            ]
          }
        },
        {
          "type": "ContactForm",
          "props": {
            "title": "Let's Plan Your Perfect Day",
            "style": "inquiry",
            "fields": ["names", "email", "weddingDate", "venue", "guestCount", "photography-style", "message"],
            "backgroundColor": "#fae8e8"
          }
        }
      ]
    },
    {
      "title": "Galleries",
      "slug": "galleries",
      "blocks": [
        {
          "type": "Gallery",
          "props": {
            "layout": "wedding-grid",
            "categories": ["Full Weddings", "Details", "Couples", "Receptions"],
            "displayStyle": "preview-cards"
          }
        }
      ]
    },
    {
      "title": "Investment",
      "slug": "investment",
      "blocks": [
        {
          "type": "Packages",
          "props": {
            "title": "Wedding Collections",
            "style": "elegant",
            "packages": [
              {
                "name": "Essential",
                "price": "$2,800",
                "features": ["6 hours coverage", "One photographer", "400+ edited images", "Online gallery"],
                "ribbon": ""
              },
              {
                "name": "Classic",
                "price": "$3,800",
                "features": ["8 hours coverage", "Two photographers", "600+ edited images", "Online gallery", "Engagement session"],
                "ribbon": "Most Popular"
              },
              {
                "name": "Luxury",
                "price": "$5,500",
                "features": ["Full day coverage", "Two photographers", "800+ edited images", "Online gallery", "Engagement session", "Premium album"],
                "ribbon": ""
              }
            ]
          }
        }
      ]
    }
  ]
}
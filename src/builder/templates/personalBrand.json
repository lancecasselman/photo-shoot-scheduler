{
  "name": "Personal Brand Influencer",
  "slug": "personal-brand-influencer",
  "preview": "/previews/personal-brand.jpg",
  "description": "Modern design for personal branding and influencer photographers",
  "tags": ["Personal Brand", "Influencer", "Social Media", "Modern"],
  "globalStyles": {
    "colors": {
      "primary": "#ec4899",
      "secondary": "#8b5cf6",
      "text": "#1f2937",
      "background": "#fdf2f8"
    },
    "fonts": {
      "heading": "Montserrat",
      "body": "Inter"
    },
    "spacing": "normal",
    "borderRadius": "large"
  },
  "pages": [
    {
      "title": "Home",
      "slug": "home",
      "blocks": [
        {
          "type": "Hero",
          "props": {
            "style": "influencer-intro",
            "headline": "Your Brand, Elevated",
            "subtext": "Photography that makes you stand out online",
            "image": "/images/influencer-hero.jpg",
            "socialProof": "10k+ influencers photographed"
          }
        },
        {
          "type": "SocialProof",
          "props": {
            "title": "As Seen On",
            "platforms": ["Instagram", "TikTok", "YouTube", "LinkedIn"],
            "style": "modern-icons"
          }
        },
        {
          "type": "Gallery",
          "props": {
            "layout": "instagram-style",
            "aspectRatio": "1:1",
            "columns": 3,
            "images": []
          }
        },
        {
          "type": "Testimonial",
          "props": {
            "style": "influencer-reviews",
            "testimonials": [
              {
                "quote": "My engagement doubled after using these photos!",
                "client": "@fashionblogger",
                "followers": "50k"
              },
              {
                "quote": "Professional photos that still feel authentic",
                "client": "@fitnessguru",
                "followers": "120k"
              }
            ]
          }
        }
      ]
    },
    {
      "title": "Packages",
      "slug": "packages",
      "blocks": [
        {
          "type": "Packages",
          "props": {
            "title": "Content Packages",
            "style": "influencer-tiers",
            "packages": [
              {
                "name": "Starter",
                "price": "$500",
                "deliverables": ["20 edited photos", "1 location", "1 outfit change"],
                "bestFor": "New creators"
              },
              {
                "name": "Growth",
                "price": "$1000",
                "deliverables": ["50 edited photos", "2 locations", "3 outfit changes", "Story content"],
                "bestFor": "Growing influencers",
                "popular": true
              },
              {
                "name": "Pro",
                "price": "$2000",
                "deliverables": ["100+ edited photos", "Multiple locations", "Unlimited outfits", "Video content"],
                "bestFor": "Established brands"
              }
            ]
          }
        }
      ]
    },
    {
      "title": "Book",
      "slug": "book",
      "blocks": [
        {
          "type": "Booking",
          "props": {
            "style": "modern-booking",
            "calendar": true,
            "fields": ["name", "instagram", "followerCount", "contentNeeds", "preferredDate"],
            "availabilityIntegration": true
          }
        }
      ]
    }
  ]
}
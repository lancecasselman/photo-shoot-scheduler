{
  "name": "Vibrant Festival",
  "slug": "vibrant-festival",
  "preview": "/previews/vibrant-festival.jpg",
  "description": "Colorful, energetic design for event and festival photographers",
  "tags": ["Festival", "Vibrant", "Events", "Colorful"],
  "globalStyles": {
    "colors": {
      "primary": "#ff006e",
      "secondary": "#3a86ff",
      "text": "#1a1a1a",
      "background": "#fff5f5"
    },
    "fonts": {
      "heading": "Poppins",
      "body": "Open Sans"
    },
    "spacing": "tight",
    "borderRadius": "large"
  },
  "pages": [
    {
      "title": "Home",
      "slug": "home",
      "blocks": [
        {
          "type": "Hero",
          "props": {
            "style": "festival-collage",
            "headline": "FESTIVAL VIBES",
            "subtext": "Capturing the energy of live events",
            "images": ["/images/fest1.jpg", "/images/fest2.jpg", "/images/fest3.jpg"],
            "animation": "pulse",
            "ctaText": "See Events",
            "ctaLink": "#events"
          }
        },
        {
          "type": "Stats",
          "props": {
            "style": "colorful",
            "stats": [
              { "number": "50+", "label": "Festivals" },
              { "number": "200k", "label": "Photos" },
              { "number": "100+", "label": "Artists" }
            ]
          }
        },
        {
          "type": "Gallery",
          "props": {
            "layout": "festival-grid",
            "effect": "color-pop",
            "autoplay": true,
            "images": []
          }
        },
        {
          "type": "ClientLogos",
          "props": {
            "title": "Featured At",
            "style": "festival-badges",
            "logos": ["Coachella", "Bonnaroo", "Lollapalooza", "EDC"]
          }
        }
      ]
    },
    {
      "title": "Events",
      "slug": "events",
      "blocks": [
        {
          "type": "EventGalleries",
          "props": {
            "layout": "festival-cards",
            "showYear": true,
            "showLineup": true,
            "filterByGenre": true
          }
        }
      ]
    },
    {
      "title": "Book",
      "slug": "book",
      "blocks": [
        {
          "type": "Packages",
          "props": {
            "title": "Event Coverage",
            "style": "vibrant-cards",
            "packages": [
              {
                "name": "Single Day",
                "price": "$1500",
                "color": "#ff006e"
              },
              {
                "name": "Weekend",
                "price": "$3500",
                "color": "#3a86ff"
              },
              {
                "name": "Full Festival",
                "price": "$6000",
                "color": "#ff006e"
              }
            ]
          }
        }
      ]
    }
  ]
}